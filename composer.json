{
    "name": "ksfraser/html",
    "version": "1.0.0",
    "description": "HTML project for Ksfraser",
    "require": {
        "php": ">=7.3",
        "ksfraser/origin": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../origin"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ksfraser\\HTML\\": "src/Ksfraser/HTML/",
            "Ksfraser\\HTML\\Elements\\": "src/Ksfraser/HTML/Elements/",
            "Ksfraser\\HTML\\CSS\\": "src/Ksfraser/HTML/CSS/",
            "Ksfraser\\HTML\\Composites\\": "src/Ksfraser/HTML/Composites/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ksfraser\\HTML\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit"
    }
}
